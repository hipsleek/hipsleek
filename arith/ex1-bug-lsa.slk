data node {
  int val;
  node nxt;
}.

pred lseg<l,y,p> == emp & self=y & l=p
  or (exists q: self::node<_,q> * q::lseg<l-1,y,p>)
inv l>=p.

checkentail x::node<_,q>*q::lseg<l,y,p> |- x::lseg<m,y,z>.
print residue.

