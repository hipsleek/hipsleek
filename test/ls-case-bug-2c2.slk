data node {
  node next;
}.

pred ls3<n> == case {
   n=0 -> [] self=null; 
   n!=0 -> case {
             n>0 -> [] self::node<q>*q::ls3<n-1> & n<(-2);
             n<0 -> [] false;
        };
  } inv n>=0.


