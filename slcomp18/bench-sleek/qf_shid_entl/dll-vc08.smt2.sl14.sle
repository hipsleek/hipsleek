
data RefDll_t {
	RefDll_t next;
	RefDll_t prev;
}.

pred dll<bk,pr,nx> ==   emp & self=nx & bk=pr
	or  exists u: self::RefDll_t<u:next,pr:prev> * u::dll<bk,self,nx> & self!=nx & bk!=pr
.


checkentail w_emp::RefDll_t<t_emp:next,u_emp:prev> * x_emp::dll<u_emp,nil,w_emp> * t_emp::dll<y_emp,w_emp,z_emp> & x_emp!=w_emp & w_emp!=t_emp & z_emp!=w_emp
	 |- x_emp::dll<y_emp,nil,z_emp>.
