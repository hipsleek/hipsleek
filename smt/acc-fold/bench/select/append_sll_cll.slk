data node {
  node next;
}.
 pred lseg<p:node> == 
  emp&self=p or
(exists p_21,q_20: self::node<q_20> * q_20::lseg<p_21>&
  p_21=p)
  inv true.
 pred ll<> == 
  emp&self=null or
(exists q_22: self::node<q_22> * q_22::ll<>)
  inv true.
