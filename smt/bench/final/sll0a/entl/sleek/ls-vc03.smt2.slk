
data Sll_t {
  Sll_t next;
}.

pred ls<out:Sll_t> ==
 self = out
or (exists u: self::Sll_t<next = u> * u::ls<out> & self != out).

checkentail x_emp::Sll_t<next = y_emp> * y_emp::ls<w_emp> * w_emp::ls<z_emp> * z_emp::Sll_t<next = u_emp> * u_emp::Sll_t<next = v_emp> * v_emp::ls<r_emp> * r_emp::Sll_t<next = s_emp>
         |- x_emp::ls<s_emp>.