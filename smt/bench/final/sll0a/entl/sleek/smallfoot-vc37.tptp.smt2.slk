
data Sll_t {
  Sll_t next;
}.

pred ls<out:Sll_t> ==
 self = out
or (exists u: self::Sll_t<next = u> * u::ls<out> & self != out).

checkentail_exact x2::ls<x1> * x3::Sll_t<next = x2> * x1::Sll_t<next = x3> * emp & null = null & null != x1 & null != x2 & null != x3 & x1 != x3 & x2 != x3
         |- x4::Sll_t<next = x3> * x2::ls<x4> * x3::Sll_t<next = x2> * emp.
expect Fail.
