
data GTyp {
  GTyp f0;
  GTyp f1;
}.

pred BinPath<y:GTyp> ==
 self = y
or (exists xp,yp: self::GTyp<f0 = xp,f1 = yp> * xp::BinPath<y> & null != self)
or (exists xp,yp: self::GTyp<f0 = xp,f1 = yp> * yp::BinPath<y> & null != self).

checkentail_exact x::BinPath<z> * z::BinPath<y>
         |- x::BinPath<y>.

expect Valid.